{
  "name": "@craftvue/icons",
  "version": "1.1.3",
  "private": false,
  "type": "module",
  "author": "Craft",
  "description": "Icons kit for CraftVue",
  "keywords": [
    "vue",
    "vue3",
    "icons",
    "components",
    "craftvue"
  ],
  "homepage": "https://matmon12.github.io/craftvue/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matmon12/craftvue.git",
    "directory": "packages/icons"
  },
  "bugs": {
    "url": "https://github.com/matmon12/craftvue/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./search": {
      "types": "./dist/components/SearchIcon.d.ts",
      "import": "./dist/components/SearchIcon.mjs",
      "require": "./dist/components/SearchIcon.cjs"
    },
    "./close": {
      "types": "./dist/components/CloseIcon.d.ts",
      "import": "./dist/components/CloseIcon.mjs",
      "require": "./dist/components/CloseIcon.cjs"
    },
    "./spinner": {
      "types": "./dist/components/SpinnerIcon.d.ts",
      "import": "./dist/components/SpinnerIcon.mjs",
      "require": "./dist/components/SpinnerIcon.cjs"
    },
    "./eye": {
      "types": "./dist/components/EyeIcon.d.ts",
      "import": "./dist/components/EyeIcon.mjs",
      "require": "./dist/components/EyeIcon.cjs"
    },
    "./eyeslash": {
      "types": "./dist/components/EyeSlashIcon.d.ts",
      "import": "./dist/components/EyeSlashIcon.mjs",
      "require": "./dist/components/EyeSlashIcon.cjs"
    },
    "./prevarrow": {
      "types": "./dist/components/PrevArrowIcon.d.ts",
      "import": "./dist/components/PrevArrowIcon.mjs",
      "require": "./dist/components/PrevArrowIcon.cjs"
    },
    "./nextarrow": {
      "types": "./dist/components/NextArrowIcon.d.ts",
      "import": "./dist/components/NextArrowIcon.mjs",
      "require": "./dist/components/NextArrowIcon.cjs"
    },
    "./check": {
      "types": "./dist/components/CheckIcon.d.ts",
      "import": "./dist/components/CheckIcon.mjs",
      "require": "./dist/components/CheckIcon.cjs"
    },
    "./edit": {
      "types": "./dist/components/EditIcon.d.ts",
      "import": "./dist/components/EditIcon.mjs",
      "require": "./dist/components/EditIcon.cjs"
    },
    "./save": {
      "types": "./dist/components/SaveIcon.d.ts",
      "import": "./dist/components/SaveIcon.mjs",
      "require": "./dist/components/SaveIcon.cjs"
    },
    "./delete": {
      "types": "./dist/components/DeleteIcon.d.ts",
      "import": "./dist/components/DeleteIcon.mjs",
      "require": "./dist/components/DeleteIcon.cjs"
    },
    "./home": {
      "types": "./dist/components/HomeIcon.d.ts",
      "import": "./dist/components/HomeIcon.mjs",
      "require": "./dist/components/HomeIcon.cjs"
    },
    "./add": {
      "types": "./dist/components/AddIcon.d.ts",
      "import": "./dist/components/AddIcon.mjs",
      "require": "./dist/components/AddIcon.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "version:patch": "npm version patch --no-git-tag-version --workspaces=false",
    "version:minor": "npm version minor --no-git-tag-version --workspaces=false",
    "version:major": "npm version major --no-git-tag-version --workspaces=false",
    "publish:patch": "pnpm run build && pnpm run version:patch && npm publish",
    "publish:minor": "pnpm run build && pnpm run version:minor && npm publish",
    "publish:major": "pnpm run build && pnpm run version:major && npm publish"
  }
}
